{
  "version": 1,
  "projects": {
    "ng-alain-app": {
      "projectType": "application",
      "schematics": {
        "@nrwl/angular:component": {
          "style": "less"
        }
      },
      "root": "apps/ng-alain-app",
      "sourceRoot": "apps/ng-alain-app/src",
      "prefix": "fs",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/ng-alain-app",
            "index": "apps/ng-alain-app/src/index.html",
            "main": "apps/ng-alain-app/src/main.ts",
            "polyfills": "apps/ng-alain-app/src/polyfills.ts",
            "tsConfig": "apps/ng-alain-app/tsconfig.app.json",
            "aot": false,
            "assets": [
              "apps/ng-alain-app/src/favicon.ico",
              "apps/ng-alain-app/src/assets",
              {
                "glob": "**/*",
                "input": "./node_modules/@ant-design/icons-angular/src/inline-svg/",
                "output": "/assets/"
              }
            ],
            "styles": [
              "apps/ng-alain-app/src/styles.less",
              "node_modules/font-awesome/css/font-awesome.css",
              "node_modules/primeng/resources/themes/nova-light/theme.css",
              "node_modules/primeicons/primeicons.css",
              "node_modules/primeng/resources/primeng.min.css"
            ],
            "scripts": [
              "node_modules/@antv/g2/build/g2.js",
              "node_modules/@antv/data-set/dist/data-set.min.js",
              "node_modules/@antv/g2-plugin-slider/dist/g2-plugin-slider.min.js",
              "node_modules/ajv/dist/ajv.bundle.js",
              "node_modules/qrious/dist/qrious.min.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/ng-alain-app/src/environments/environment.ts",
                  "with": "apps/ng-alain-app/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "ng-alain-app:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "ng-alain-app:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "ng-alain-app:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/ng-alain-app/tsconfig.app.json",
              "apps/ng-alain-app/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!apps/ng-alain-app/**"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/ng-alain-app/jest.config.js",
            "tsConfig": "apps/ng-alain-app/tsconfig.spec.json",
            "setupFile": "apps/ng-alain-app/src/test-setup.ts"
          }
        }
      }
    },
    "fs.identity": {
      "projectType": "library",
      "root": "libs/identity",
      "sourceRoot": "libs/identity/src",
      "prefix": "fs",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "libs/identity/tsconfig.lib.json",
            "project": "libs/identity/ng-package.json"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/identity/tsconfig.lib.json",
              "libs/identity/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!libs/identity/**"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/identity/jest.config.js",
            "tsConfig": "libs/identity/tsconfig.spec.json",
            "setupFile": "libs/identity/src/test-setup.ts"
          }
        }
      },
      "schematics": {}
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "schematics": {
    "@nrwl/angular:application": {
      "unitTestRunner": "jest",
      "e2eTestRunner": "none"
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "jest"
    }
  },
  "defaultProject": "ng-alain-app"
}
