<h2>
  {{ '<%= className %>::Menu:Lock' | abpLocalization }} -
  {{ stateService.getSelected()?.userName }}
</h2>

<hr class="my-3" />

<form
  *ngIf="lockForm"
  class="abp-md-form"
  [formGroup]="lockForm"
  id="PasswordForm"
  (ngSubmit)="setPassword()"
  [validateOnSubmit]="true"
>
  <label class="form-label">{{
    'AbpIdentity::DisplayName:LockoutEnd' | abpLocalization
  }}</label>
  <div class="mt-2 fade-in-top">
    <div class="mb-3" [formGroup]="lockForm">
      <abp-date-time-picker [prop]="dateTimePickerProps"></abp-date-time-picker>
    </div>
  </div>

  <hr />

  <button
    type="submit"
    class="btn btn-primary"
    [disabled]="lockForm.invalid || saving"
  >
    {{ '<%= className %>::Save' | abpLocalization }}
  </button>
</form>
