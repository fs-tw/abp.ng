<h2>
  {{ '<%= namePathName %>::Menu:<%= className %>' | abpLocalization }} - 
  {{ stateService.getSelected()?.userName }}
</h2>

<hr class="my-3" />

<form
  *ngIf="<%= propertyName %>Form"
  class="abp-md-form"
  [formGroup]="<%= propertyName %>Form"
  id="PasswordForm"
  (ngSubmit)="save()"
  [validateOnSubmit]="true"
>


  <hr />

  <button
    type="submit"
    class="btn btn-primary"
    [disabled]="saving"
  >
    {{ '<%= namePathName %>::Save' | abpLocalization }}
  </button>
</form>
